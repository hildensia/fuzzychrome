{
  "name": "fuzzychrome",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.com",
  "homepage_url": "http://www.github.com/hildensia/fuzzychrome",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "lib/fuse.min.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "omnibox": {
    "keyword": "t"
  }
}
